{
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": [
        "apps/cert-manager/production/config_dir.json"
      ],
      "matchStrings": [
        "\"chartVersion\": \"(\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "cert-manager",
      "versioning": "regex",
      "sourceUrl": "https://github.com/victorbiga/test-renovate",
      "tagPrefix": "cert-manager-v"
    }
  ],
  "dependencyDashboard": true
}
