{
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchUpdateTypes": ["version"],
      "updateTypes": ["github-tags"],
      "datasourceTemplate": "github-tags",
      "depName": "cert-manager",
      "versioning": "regex",
      "sourceUrl": "https://github.com/victorbiga/test-renovate",
      "tagPrefix": "cert-manager-v",
      "fileMatch": [
        "apps/cert-manager/production/config_dir.json",
      ]
    }
  ],
  "dependencyDashboard": true
}
